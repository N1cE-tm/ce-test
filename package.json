{
	"name": "n1ce.test",
	"description": "A base template for building a shareable web components library with Vite + Svelte",
	"version": "0.0.0",
	"type": "module",
	"module": "dist/lib/n1ce.test.js",
	"main": "dist/lib/n1ce.test.umd.js",
	"scripts": {
		"start": "npm run dev -s",
		"dev": "vite --config vite.demo.config.ts",
		"build": "vite build",
		"build:demo": "vite build --config vite.demo.config.ts",
		"preview": "vite preview --config vite.demo.config.ts",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"prepublishOnly": "npm run build -s"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.5",
		"@tsconfig/svelte": "^5.0.0",
		"sass": "^1.65.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.9",
		"vite-plugin-filter-replace": "^0.1.12"
	},
	"files": [
		"dist/lib"
	]
}
